#!/bin/bash
awk 'BEGIN{x="water";y="stir.";z="bestchol";for(i=0;i<5;i++)a[substr(x,i+1,1)]=i;for(i=0;i<5;i++)b[substr(y,i+1,1)]=i;n=0;m=0;for(i=0;i<length(ENVIRON["WATER"]);i++)n=n*5+a[substr(ENVIRON["WATER"],i+1,1)];for(i=0;i<length(ENVIRON["STIR"]);i++)m=m*5+b[substr(ENVIRON["STIR"],i+1,1)];t=n+m;r="";while(t>0){r=substr(z,t%9+1,1)r;t=int(t/9)}print r}'
